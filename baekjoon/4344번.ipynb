   "source": [
    "n = int(input())\n",
    "for _ in range(n):\n",
    "    a = list(map(int,input().split()))\n",
    "    means=sum(a[1:])/a[0]\n",
    "    cnt = 0\n",
    "    for i in a[1:]:\n",
    "        if i > means :\n",
    "            cnt += 1\n",
    "    비율 = (cnt/len(a[1:]))*100\n",
    "    print(f'{비율:0.3f}%')"
   ]
