   "source": [
    "def bubble(arr):\n",
    "    for i in range(len(arr),-1,-1):\n",
    "        for j in range(i-1):\n",
    "            if arr[j] > arr[j+1]:\n",
    "                arr[j] , arr[j+1] = arr[j+1], arr[j]    \n",
    "    return arr           "
   ]
  },
